local sound = Instance.new("Sound")
sound.Parent = game.Workspace

local musicIds = {
    "http://www.roblox.com/asset?id=1843514537",
    "http://www.roblox.com/asset?id=9046864489",
    "http://www.roblox.com/asset?id=9046863235",
