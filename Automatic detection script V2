    "http://www.roblox.com/asset?id=1845554017",

}


local randomMusicId = musicIds[math.random(1, #musicIds)]


sound.Name = "Music"
sound.SoundId = randomMusicId
sound.Volume = 1
sound.Looped = true

sound:Play()


wait(250)

sound:Stop()
