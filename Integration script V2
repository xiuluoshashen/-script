local player = game.Players.LocalPlayer
local scriptUrl = userScripts[player.Name]

if not scriptUrl then
    warn("你没有执行此脚本的权限。")
    return
end

-- 加载对应的脚本
loadstring(game:HttpGet(scriptUrl))()
