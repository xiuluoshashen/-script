local allowedUsers = {
    "Miisnoch"
}
 
local player = game.Players.LocalPlayer
 
local isAllowed = false
for i, username in ipairs(allowedUsers) do
    if player.Name == username then
        isAllowed = true
        break
    end
end
 
if not isAllowed then
    warn("ä½ æ²¡æœ‰æ‰§è¡Œæ­¤è„šæœ¬çš„æƒé™ã€‚")
    return
end
